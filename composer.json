{
    "name": "cenarioweb/toolkit",
    "description": "A toolkit of functions for general use",
    "type": "library",
    "require": {
        "php": ">=5.6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Eduardo",
            "email": "carloseduardo@cenarioweb.com.br"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "CenarioWeb\\Toolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CenarioWeb\\ToolkitTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Toolkit": "CenarioWeb\\Toolkit"
            }
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always"
    }
}
